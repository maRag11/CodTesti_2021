<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<!-- Esercizio 5-->

<!--Riferimento al filedtd.dtd dove sono presenti i vincoli -->
<!DOCTYPE TEI SYSTEM "./filedtd.dtd">

 <!--Elemento root-->
 <TEI> 
    <!--Definisco gli elementi figli (header, facsimile, text)-->
    <!--Inserisco gli attributi specificati nel testo della consegna-->
    <header type="intestazione" lang="ITA">
        Intestazione
    </header>
    <facsimile source="fonte">
        Facsimile
    </facsimile>
    <text id="testo1">
        Testo
        <!--Possibile elemento <seg> figlio di <text>-->
        <seg>
            Segmento
        </seg>
        Altro testo
    </text>
</TEI>